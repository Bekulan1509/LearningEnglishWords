<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.twodev.learningenglishwords">

    <application
        android:name=".Application.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearningEnglishWords">
        <activity android:name=".ui.PutTitleActivity.PutTitleActivity"></activity>
        <activity android:name=".ui.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.PutDataActivity.PutDataActivity" />
        <activity
            android:name=".ui.InputDataActivity"
            android:label="@string/title_activity_input_data"
            android:theme="@style/Theme.LearningEnglishWords.NoActionBar" />
        <activity android:name=".TEstActivity" />
        <activity android:name=".ui.ItemsActivity.ItemsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>